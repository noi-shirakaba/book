<h1>書籍の新規登録</h1>
<%= form_with model: @book do |form| %>
   <div>
    <%= form.label :title, "書籍名" %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :author, "著者名" %>
    <%= form.text_field :author %>
  </div>

  <div>
    <%= form.label :label, "ラベル" %>
    <%= form.text_field :label %>
  </div>

  <div>
    <%= form.submit "書籍を登録する" %>
  </div>
<% end %>

